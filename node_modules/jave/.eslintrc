{
    "env": {
        "node":    true,
        "browser": true,
        "amd":     true
    },
    "rules": {
        "no-multi-spaces": 0,
        "quotes": [1, "single"],
        "global-strict": [2, "never"],
        "key-spacing": [1, { "align": "value" }],
        "no-underscore-dangle": false,
        "no-mixed-spaces-and-tabs": 2,
        "vars-on-top": 1,
        "block-scoped-var": 2,
        "no-inner-declarations": 2,
        "valid-jsdoc": 1,
        "no-irregular-whitespace": 2,
        "spaced-line-comment": [1, "always"]
    }
}
